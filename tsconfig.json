{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"outDir": "dist",
		"lib": ["es2019", "es2017", "es7", "es6"],
		"esModuleInterop": true,
		"declaration": true,
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": false,
		"strictNullChecks": false
	},
	"exclude": [
		"node_modules",
		"dist",
		"test"
	]
}
